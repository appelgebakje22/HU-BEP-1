<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="259.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.appelgebakje22.bep.prc_2_3.client.ChatAppController">
    <children>
        <TextArea fx:id="chatBox" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="450.0" />
        <Label layoutX="14.0" layoutY="226.0" text="name:" />
        <TextField fx:id="chatName" layoutX="51.0" layoutY="222.0" prefHeight="25.0" prefWidth="69.0" />
        <Label layoutX="128.0" layoutY="226.0" text="message:" />
        <TextField fx:id="chatField" layoutX="181.0" layoutY="222.0" onAction="#sendMessage" prefHeight="25.0" prefWidth="230.0" />
        <Button fx:id="submit" layoutX="421.0" layoutY="222.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
    </children>
</AnchorPane>
